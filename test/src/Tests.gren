    getSecureContext
        |> Task.mapError (\_ -> Crypto.AesCtrEncryptionError)
        |> Task.andThen (\secureContext ->
            generateAesCtrKey aesKeyLength
                |> Task.mapError (\_ -> Crypto.AesCtrEncryptionError)
                |> Task.andThen (\{ key } ->
                    counterTask
                        |> Task.mapError (\_ -> Crypto.AesCtrEncryptionError)
                        |> Task.andThen (\counter ->
                            Crypto.encryptWithAesCtr secureContext { counter = counter, length = length } key bytes
                        )
                )
        )


