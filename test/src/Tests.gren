signWithHmacTests string func =
    let
        testLabel int =
            string ++ String.fromInt int

        expectSuccess int =
            expectSuccessHelper (testHelper func) (testLabel int)
        
        testBytes =
            Bytes.Encode.encode (Bytes.Encode.string "hi")  
    in
    [ expectSuccess 1 (signWithHmac Crypto.Sha256 Nothing testBytes)
    , expectSuccess 2 (signWithHmac Crypto.Sha384 Nothing testBytes)
    , expectSuccess 3 (signWithHmac Crypto.Sha512 Nothing testBytes)
    ]


verifyWithHmacTests string func =
    let
        testLabel int =
            string ++ String.fromInt int

        expectSuccess int =
            expectSuccessHelper (testHelper func) (testLabel int)
        
        testBytes =
            Bytes.Encode.encode (Bytes.Encode.string "hi")  
    in
    [ expectSuccess 1 (verifyWithHmac Crypto.Sha256 Nothing testBytes)
    , expectSuccess 2 (verifyWithHmac Crypto.Sha384 Nothing testBytes)
    , expectSuccess 3 (verifyWithHmac Crypto.Sha512 Nothing testBytes)
    ]


