{-|-}
encryptWithRsaOaep keyHash label bytes =
    getSecureContext
        |> Task.mapError (\_ -> Crypto.RsaOaepEncryptionError)
        |> Task.andThen (\secureContext ->
            generateRsaOaepKeyPair 4096 keyHash
                |> Task.mapError (\_ -> Crypto.RsaOaepEncryptionError)
                |> Task.andThen (\{ key } ->
                    Crypto.encryptWithRsaOaep secureContext { label = label } key.publicKey bytes
                )
        )


