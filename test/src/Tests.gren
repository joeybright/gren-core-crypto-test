importKey errorMap exportFunc importFunc =
    exportFunc
        |> Task.mapError (\_ -> errorMap)
        |> Task.andThen
                (\data ->
                    importFunc data
                        |> Task.map
                                (\importedKey ->
                                    { key = data.key
                                    , secureContext = data.secureContext
                                    , exportedKey = data.exportedKey
                                    , importedKey = importedKey
                                    }
                                )
                )



