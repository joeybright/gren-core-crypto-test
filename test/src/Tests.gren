-- VERIFY


verifyWithRsaSsaPkcs1V1_5 modulusLength digestAlgorithm extractable bytes = 
    getSecureContext
        |> Task.mapError (\_ -> {})
        |> Task.andThen (\secureContext ->
            Crypto.generateRsaSsaPkcs1V1_5KeyPair 
                secureContext 
                { modulusLength = modulusLength
                , hash = digestAlgorithm
                , extractable = extractable
                }
                |> Task.mapError (\_ -> {})
                |> Task.andThen (\{ privateKey, publicKey } ->
                    Crypto.signWithRsaSsaPkcs1V1_5 secureContext privateKey bytes
                        |> Task.mapError (\_ -> {})
                        |> Task.andThen (\signature ->
                            Crypto.verifyWithRsaSsaPkcs1V1_5 secureContext publicKey signature bytes
                        )
                )
        )


